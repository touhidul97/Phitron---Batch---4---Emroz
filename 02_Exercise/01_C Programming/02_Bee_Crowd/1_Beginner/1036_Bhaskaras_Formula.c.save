#include <stdio.h>
#include <math.h>

int main()
{
    double a, b, c, r1, r2;
    scanf("%lf%lf%lf", &a, &b, &c);
    if ((sqrt(b * b - 4 * a * c))<0)
    {
        printf("Impossivel calcular\n");
    }
    else if (a==0)
    {
        printf("Impossivel calcular\n");
        }
    else
    {
        r2 = (-b - (sqrt(b * b - 4 * a * c))) / (2 * a);
        r1 = (-b + (sqrt(b * b - 4 * a * c))) / (2 * a);
        printf("R1 = %.5lf\n", r1);
        printf("R2 = %.5lf\n", r2);
    }
    return 0;
}
